<Window x:Class="MoodMusic.UI.Auth"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoodMusic.UI"
        mc:Ignorable="d"
        Title="Auth" Height="400" Width="400" Closing="Window_Closing" >
    <Grid Loaded="Loaded_Window">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <WebBrowser  x:Name="webBrowser" Grid.ColumnSpan="2" Grid.RowSpan="2"    Navigating="webBrowser_Navigating" LoadCompleted="webBrowser_LoadCompleted"/>

    </Grid>
</Window>
